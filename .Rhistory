install.packages("UsingR")
library(UsingR)
data("father.son")
plot(father.son$fheight,father.son$sheight,xlab="Father's height (in")
ylab="Son's height (in)",pch=20;
ylab="Son's height (in)", pch=20);
ylab="Son\'s height (in)", pch=20);
ylab="Son\'s height (in)", pch=20)
ylab="Son\'s height (in)", pch=20
plot(father.son$fheight,father.son$sheight,xlab="Father's height (in)",ylab = "Son's height (in)", pch=20)
model <- lm(father.son$sheight ~ father.son$fheight)
abline()
abline(model, col="blue", lwd="2")
abline(model, col="red", lwd="2")
abline(a=a,b=b,col="blue",lwd="3" )
a <- coefficients(model)[1]
b <- coefficients(model)[2]
abline(a=a,b=b,col="blue",lwd="3" )
> abline(model, col="red", lwd="2")
abline(model, col="red", lwd="2")
dev.off()
dev.new()
plot(father.son$fheight,father.son$sheight,xlab="Father's height (in)",ylab = "Son's height (in)", pch=20)
abline(model, col="red", lwd="2")
a <- coefficients(model)[1]
b <- coefficients(model)[2]
abline(a=a, b=b, col="blue", lwd=3)
residual_sd <- sd(model$residuals)
abline(a = a + residual_sd, b = b, col = "lightblue", lwd = 2, lty = 2)
abline(a = a - residual_sd, b = b, col = "lightblue", lwd = 2, lty = 2)
abline(model, col="red", lwd="2")
residual_sd <- sd(model$residuals)
abline(a = a + residual_sd, b = b, col = "blue", lwd = 2, lty = 2)
abline(a = a - residual_sd, b = b, col = "blue", lwd = 2)
abline(a = a + residual_sd, b = b, col = "blue", lwd = 2)
mean_fheight <- mean(father.son$fheight)
mean_sheight <- a + b * mean_fheight
points(mean_fheight, mean_sheight, col="purple", pch=19, cex=1.5)
abline(v = mean_fheight, col = "green", lty = 2, lwd = 2)
abline(h = mean_sheight, col = "green", lty = 2, lwd = 2)
model <- lm(father.son$sheight ~ father.son$fheight)
summary(model)
